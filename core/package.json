{
  "name": "@erda-ui/core",
  "version": "1.0.0",
  "description": "Core functions for Erda UI",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "start": "webpack serve",
    "build-dev": "cross-env-shell NODE_ENV=production FOR_DEV=true 'npm run build-core'",
    "build": "cross-env-shell NODE_ENV=production 'npm run build-core'",
    "build-core": "rm -rf ../public/static/core/ && node ./node_modules/.bin/webpack --progress",
    "analyze": "node --max-old-space-size=4048 node_modules/.bin/webpack --config webpack.analyze.js --progress --color",
    "serve": "serve public -p 5000",
    "clean": "rm -rf public"
  },
  "author": "Erda-FE",
  "license": "AGPL",
  "dependencies": {
    "@terminus/nusi": "^3.10.5",
    "antd": "^3.25.0",
    "css-loader": "^5.0.1",
    "cube-state": "^1.3.0",
    "i18next": "^17.0.13",
    "immer": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^7.2.1",
    "moment": "^2.29.1",
    "path-to-regexp": "^1.7.0",
    "postcss-loader": "^4.1.0",
    "query-string": "^6.4.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^10.1.1",
    "sockjs-client": "^1.1.5",
    "superagent": "^5.2.2",
    "superagent-use": "^0.1.0",
    "ts-import-plugin": "^1.4.4",
    "ts-loader": "^8.0.14",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/read": "^7.5.0",
    "@module-federation/automatic-vendor-federation": "^1.2.1",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.18",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.1",
    "@types/react-router-config": "^1.1.3",
    "@types/react-router-dom": "^5.1.5",
