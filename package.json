{
  "name": "erda-ui",
  "version": "2.1.0",
  "description": "Frontend ui for Erda",
  "private": true,
  "scripts": {
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "install-cli": "node ./install-cli.js",
    "update-version": "erda-ui set-pkg-version",
    "clear-dependencies": "erda-ui clear-dependencies",
    "init": "erda-ui init",
    "build": "erda-ui build --online",
    "build-online": "erda-ui build --online",
    "i18n": "erda-ui i18n",
    "preinstall": "npx only-allow pnpm",
    "pnpm:devPreinstall": "mkdir -p ./cli/dist/bin && touch ./cli/dist/bin/erda.js",
    "postinstall": "pnpm --filter @erda-ui/components run build:noDeclare  && pnpm --filter @erda-ui/cli run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erda-project/erda-ui.git"
  },
  "keywords": [
    "erda-ui"
  ],
  "author": "Erda-FE",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/erda-project/erda-ui/issues"
  },
  "homepage": "https://github.com/erda-project/erda-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@babel/runtime": "^7.22.11",
    "@commitlint/cli": "^12.1.4",
    "@erda-ui/cli": "workspace:^2.0.2",
    "@module-federation/automatic-vendor-federation": "^1.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "~12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^17.0.45",
    "@types/react": "~16.14.46",
    "@types/react-dom": "~16.9.19",
    "@types/react-router": "~5.1.20",
    "@types/react-router-config": "^1.1.3",
    "@types/react-router-dom": "~5.3.3",
    "@types/superagent": "^4.1.18",
    "@types/webpack": "^4.41.33",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "~9.8.8",
    "babel-loader": "^8.3.0",
    "babel-plugin-import": "^1.13.8",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "core-js": "^3.32.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cz-customizable": "^6.9.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "f2elint": "^1.2.3",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-webpack-plugin": "^5.5.3",
    "husky": "6.0.0",
    "ignore-loader": "^0.1.2",
    "inquirer": "^8.2.6",
    "jest": "~27.5.1",
    "less": "^4.2.0",
    "less-loader": "^10.2.0",
    "lint-staged": "^11.2.6",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.28",
    "postcss-loader": "^6.2.1",
    "prettier": "2.6.2",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "rimraf": "^3.0.2",
    "sass": "^1.66.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "stylelint-config-ali": "^0.4.2",
    "thread-loader": "^3.0.4",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "2.6.13",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.9.0"
  },
  "lint-staged": {
    "shell/app/modules/extra/entry.js": "git checkout shell/app/modules/extra/entry.js",
    "*.{js,jsx,ts,tsx,json,scss,css}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "cube-state": "^1.5.6",
    "immer": "^9.0.21",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "path-to-regexp": "^6.2.1",
    "query-string": "^7.1.3",
    "superagent": "^7.1.5",
    "superagent-use": "^0.1.0",
    "tailwindcss": "^3.3.3"
  }
}
