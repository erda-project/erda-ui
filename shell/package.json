{
  "name": "@erda-ui/shell",
  "version": "2.1.0",
  "description": "Frontend ui for Erda",
  "directories": {
    "test": "test"
  },
  "private": true,
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "test:watch": "jest --verbose --watch",
    "auto-test": "npx playwright test --config=auto_test/playwright.config.ts --project=all",
    "debug-test": "PWDEBUG=1 npx playwright test --config=auto_test/playwright.config.ts --project=all",
    "gen-test": "npx playwright codegen --lang=en-US --load-storage=auto_test/auth/Manager.json https://erda.daily.terminus.io/",
    "copy:coverage": "cp -R ./coverage ../coverage",
    "watch": "cross-env-shell NODE_ENV=development 'webpack --progress --color -w'",
    "dev-build": "cross-env-shell NODE_ENV=development 'pnpm run build-app'",
    "build": "cross-env-shell NODE_ENV=production --max_old_space_size=4096 'pnpm run build-app'",
    "build-app": "webpack --progress",
    "commit": "cz-customizable",
    "analyze": "webpack --config webpack.analyze.js --progress --color -w",
    "fix-memory-limit": "cross-env-shell LIMIT=4096 increase-memory-limit",
    "lint-fix": "eslint --fix app --ext '.js,.jsx,.tsx,.ts,.d.ts'",
    "gen-sni": "node ./tools/gen-snippets",
    "gen-cp-md": "node ./tools/gen-cp-md",
    "gen-cp-go": "node ./tools/gen-cp-go",
    "gen-cp-all": "pnpm run gen-cp-md && pnpm run gen-cp-go",
    "precommit": "lint-staged",
    "check-unit-test": "node -e 'require(\"./tools/testcase-checker.js\").checkTestCase()'",
    "extra-logic": "node ../../erda-ui-enterprise/extra-job.js",
    "unused-file-detect": "ts-node --project tools/tsconfig.json tools/detect-unused-files.ts",
    "dev": "cross-env-shell NODE_ENV=development 'vite'",
    "preview": "vite preview",
    "vite-build": "vite build"
  },
  "author": "Erda-FE",
  "license": "AGPL",
  "dependencies": {
    "@erda-ui/components": ">=0.1.0",
    "@erda-ui/dashboard-configurator": "2.0.15",
    "@erda-ui/react-markdown-editor-lite": "^1.4.9",
    "antd": "4.16.13",
    "ace-builds": "^1.4.7",
    "ansi_up": "^5.0.1",
    "clipboard": "^2.0.8",
    "cronstrue": "^1.84.0",
    "dagrejs": "^0.2.1",
    "diff-match-patch": "^1.0.4",
    "dompurify": "^2.2.7",
    "echarts": "^4.2.0-rc.2",
    "element-resize-event": "^2.0.7",
    "events": "^3.3.0",
    "file-loader": "^6.0.0",
    "https-browserify": "^1.0.0",
    "i18next-scanner": "^3.1.0",
    "intersection-observer": "^0.5.1",
    "js-base64": "^3.6.1",
    "js-yaml": "^4.1.0",
    "micromark": "^3.0.10",
    "micromark-extension-gfm": "^2.0.1",
    "mock-json-schema": "^1.0.8",
    "openapi-types": "^9.1.0",
    "path-browserify": "^1.0.1",
    "query-string": "^6.4.2",
    "rc-resize-observer": "^1.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-ace": "^8.0.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-flame-graph": "^1.4.0",
    "react-flow-renderer": "^9.6.11",
    "react-markdown": "^7.1.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.5",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "requestidlecallback-polyfill": "^1.0.2",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.1.1",
    "screenfull": "^5.0.2",
    "snapsvg-cjs": "0.0.6",
    "stream-http": "^3.2.0",
    "style-loader": "^2.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "swagger-parser": "^10.0.2",
    "tiny-pinyin": "^1.3.2",
    "xterm": "3.12.0"
  },
  "optionalDependencies": {
    "@playwright/test": "^1.14.0",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.4.6",
    "chalk": "^4.1.1",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest-canvas-mock": "1.1.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "mockdate": "^3.0.5",
    "mockjs": "^1.0.1-beta3",
    "mq-polyfill": "^1.1.8",
    "playwright": "^1.14.0",
    "react-test-renderer": "^17.0.2",
    "tsconfig-paths-jest": "0.0.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.14.7",
    "@babel/traverse": "^7.14.7",
    "@paiva/translation-google": "^1.0.9",
    "@phenomnomnominal/tsquery": "^4.1.1",
    "@types/diff-match-patch": "^1.0.32",
    "@types/echarts": "^4.1.3",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@vitejs/plugin-legacy": "^1.4.4",
    "@vitejs/plugin-react": "1.0.5",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "babel-plugin-tsconfig-paths": "^1.0.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "event-hooks-webpack-plugin": "^2.2.0",
    "fast-glob": "^3.2.7",
    "filemanager-webpack-plugin": "^3.1.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "increase-memory-limit": "^1.0.7",
    "node-notifier": "^10.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier-stylelint": "^0.4.2",
    "process": "^0.11.10",
    "string.prototype.replaceall": "^1.0.5",
    "swagger-parser-mock": "^1.1.1",
    "tsconfig-paths": "^3.10.1",
    "webpack-build-notifier": "^2.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,scss,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "browserslist": [
    "last 5 major versions and since 2015 and > 1%",
    "not samsung > 0 and not op_mini all and not and_chr > 0 and not and_uc > 0 and not ios_saf > 0"
  ]
}
