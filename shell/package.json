{
  "name": "@erda-ui/shell",
  "version": "1.0.0",
  "description": "Frontend ui for Erda",
  "directories": {
    "test": "test"
  },
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "test:watch": "jest --verbose --watch",
    "copy:coverage": "cp -R ./coverage ../coverage",
    "dev": "node_modules/webpack/bin/webpack.js serve",
    "start": "node_modules/webpack/bin/webpack.js serve",
    "watch": "rm -rf ../public/shell/scripts && NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --color -w",
    "build": "cross-env-shell NODE_ENV=production 'npm run build-app'",
    "build-app": "rm -rf ../public/static/shell && node ./node_modules/.bin/webpack --progress",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "analyze": "rm -rf ../public/shell && node --max-old-space-size=4048 node_modules/.bin/webpack --config webpack.analyze.js --progress --color -w",
    "fix-memory-limit": "cross-env-shell LIMIT=4096 increase-memory-limit",
    "login": "./bash/login-ssh.sh",
    "lint-fix": "eslint --fix app --ext '.js,.jsx,.tsx,.ts'",
    "stylelint-fix": "stylelint 'app/**/*.scss' --config .stylelintrc.json --fix",
    "changelog": "conventional-changelog -n tools/changelog-config.js -i CHANGELOG.md -s",
    "extract": "node ./tools/extract-zh-words.js",
    "locale": "node -e 'require(\"./tools/i18n-extract.js\").writeLocale()'",
    "lint": "lint-staged",
    "gen-sni": "node ./tools/gen-snippets",
    "gen-cp-md": "node ./tools/gen-cp-md",
    "gen-cp-go": "node ./tools/gen-cp-go",
    "gen-cp-all": "npm run gen-cp-md && npm run gen-cp-go",
    "translate": "node -e 'require(\"./tools/google-translate.js\").doTranslate()'",
    "iconfont": "node -e 'require(\"./tools/manage-iconfont.js\").manageIconFont()'",
    "restore-iconfont": "node -e 'require(\"./tools/manage-iconfont.js\").restoreIconFont()'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --reporter mochawesome",
    "cypress:report": "mochawesome-merge \"cypress/temp/*.json\" > cypress/reports/mochawesome.json &&  npx marge  cypress/reports/mochawesome.json -o cypress/reports"
  },
  "author": "Erda-FE",
  "license": "AGPL",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
