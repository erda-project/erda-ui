{
  "extends": "./tsconfig-webpack.json",
  "compilerOptions": {
    "composite": true,
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "incremental": true,
    "strict": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": false
  }
}
