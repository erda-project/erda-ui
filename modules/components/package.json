{
  "name": "@erda-ui/components",
  "version": "0.1.2",
  "description": "erda ui component library",
  "scripts": {
    "start": "dumi dev",
    "build:dist": "webpack",
    "build": "gulp compile",
    "build:watch": "gulp --gulpfile gulpfile-build.js --watch",
    "build:noDeclare": "gulp --gulpfile gulpfile-build.js",
    "docs:build": "dumi build",
    "prettier": "prettier -c --write .",
    "precommit": "lint-staged",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "test": "jest",
    "prepare": "npm run build:noDeclare",
    "test:coverage": "jest --coverage",
    "publish:patch": "npm run build && npm version patch && npm publish --registry https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/McDaddy/erda-ui-components.git"
  },
  "homepage": "https://mcdaddy.github.io/erda-ui-components/",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/components.js",
  "umd:main": "dist/components.js",
  "typings": "lib/index.d.ts",
  "sideEffects": [
    "dist/*",
    "es/*.js",
    "lib/*.js",
    "src/*.ts",
    "*.less",
    "**/*/style/index.js"
  ],
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "keywords": [
    "antd",
    "react",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "author": "Erda-FE",
  "license": "MIT",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11",
    "not IE 10"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@formily/antd": "^2.0.18",
    "@formily/core": "^2.0.18",
    "@formily/react": "^2.0.18",
    "@formily/reactive": "^2.0.18",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "antd": "^4.19.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cz-customizable": "^6.3.0",
    "dumi": "^1.1.40",
    "dumi-theme-default": "^1.1.21",
    "eslint": "^8.3.0",
    "eslint-plugin-markdown": "^2.2.1",
    "f2elint": "^2.2.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^4.0.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.21",
    "merge2": "^1.4.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "stylelint-config-ali": "^0.4.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "peerDependencies": {
    "antd": ">=4.x",
    "lodash": ">=4.x",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
