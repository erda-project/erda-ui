{
  "name": "erda-ui-market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "npm run clean && npm run build-app",
    "build-app": "craco build",
    "test": "craco test",
    "copy": "erda-ui copy market",
    "eject": "react-scripts eject",
    "clean": "rm -rf dist",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.15.4",
    "craco-antd": "^1.19.0",
    "file-loader": "^6.2.0",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.18.1",
    "qrcode.react": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "superagent": "^6.1.0",
    "thread-loader": "^3.0.3",
    "typescript": "^4.2.4",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/qrcode.react": "^1.0.1",
    "autoprefixer": "^9.8.6",
    "babel-plugin-import": "^1.13.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,scss,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
